<mvc:View controllerName="FioriTilesfor10Projects.projects10.controller.Tiles"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout">
    <Page id="tiles" showHeader="false">
    <l:VerticalLayout>
    <l:HorizontalLayout content="{tiles>/}" allowWrapping="true">
    <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{tiles>title}"
    subheader="{tiles>subtitle}" backgroundImage="{tiles>backgroundImage}" press="press(${tiles>route})">
	</GenericTile>
    </l:HorizontalLayout>
    </l:VerticalLayout>
    </Page>
    
</mvc:View>
